"GE Quiz Question": {
  "prefix": "geQuizQues",
  "body": [
    "<!DOCTYPE html>",
    "<html>",
    "	<head>",
    "		<meta charset=\"UTF-8\">",
    "		<title>Quiz Template</title>",
    "		<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\">",
    "		<link rel=\"stylesheet\" href=\"https://fonts.googleapis.com/css?family=Roboto:100,200,300,400,500|Roboto+Slab|Material+Icons\" type=\"text/css\" />",
    "		<link rel=\"stylesheet\" href=\"https://code.getmdl.io/1.3.0/material.blue_grey-blue.min.css\" />",
    "		<link rel=\"stylesheet\" href=\"https://geteach.com/scripts/newearth/geteachStyle1227.css\" type=\"text/css\" />",
    "		<script>",
    "			function answerClick(){",
    "				document.getElementById('REPLACEA1show').click();",
    "				document.getElementById('REPLACEA1go').click();",
    "			};",
    "			window.onload = function(){ ",
    "				document.getElementById('REPLACEA1hide').click();",
    "				document.getElementById('REPLACEA2hide').click();	",
    "				document.getElementById('REPLACEA3hide').click();					",
    "			}",
    "		</script>",
    "	</head>",
    "	<body>",
    "		<div id=\"container\">",
    "			<main class=\"panelMain\">",
    "				<div class=\"mediaContain\">",
    "					<div class=\"slideshow-container\">",
    "						<div class=\"mySlides fade\">",
    "							<img class=\"image\" src=\"${7:REPLACEimageURL}\" />",
    "							<div class=\"captionDiv\">",
    "								<div class=\"caption\">Image Source: <a href=\"${8:REPLACEimageSourceURL}\" target=\"_blank\">${9:REPLACEimageSourceTitle}</a></div>",
    "					  		</div>",
    "					    </div>",
    "					</div>",
    "				</div>",
    "				<div class=\"content\">",
    "					<div class=\"title\">",
    "						${1:REPLACETilte}",
    "					</div>",
    "					<div class=\"snippet\">",
    "						${2:REPLACESnippet}",
    "					</div>",
    "					<div class=\"description\">",
    "						<div class=\"Qtext\">",
    "							${3:REPLACEQuestion}",
    "						</div>",
    "						<button id=\"buttonA\" onclick=\"answerClick()\" class=\"mdl-chip mdl-chip--contact\">",
    "							<span class=\"mdl-chip__contact mdl-color-text--white\">",
    "								<span>A</span>",
    "							</span>",
    "							<span class=\"mdl-chip__text\">${4:REPLACEResponse1}</span>",
    "						</button>",
    "						<button id=\"buttonB\" onclick=\"answerClick()\" class=\"mdl-chip mdl-chip--contact\">",
    "							<span class=\"mdl-chip__contact mdl-color-text--white\">",
    "								<span>B</span>",
    "							</span>",
    "							<span class=\"mdl-chip__text\">${5:REPLACEResponse2}</span>",
    "						</button>",
    "						<button id=\"buttonC\" onclick=\"answerClick()\" class=\"mdl-chip mdl-chip--contact\">",
    "							<span class=\"mdl-chip__contact mdl-color-text--white\">",
    "								<span>C</span>",
    "							</span>",
    "							<span class=\"mdl-chip__text\">${6:REPLACEResponse3}</span>",
    "						</button>",
    "						<br>",
    "					</div>",
    "			    </div>",
    "				<div class=\"footer\">",
    "					<div class=\"logoLeft\">",
    "						<a href=\"https://twitter.com/geteach\" target=\"_blank\">",
    "							<img src=\"https://storage.googleapis.com/geteachkml4/logogeteachtemp.png\" >",
    "						</a>",
    "					</div>",
    "					<div class=\"logoRight\" style=\"display:none\">",
    "						<a href=\"#REPLACEIDAnswer1;show\" id=\"A1show\"></a>",
    "						<a href=\"#REPLACEIDAnswer2;show\" id=\"A2show\"></a>",
    "						<a href=\"#REPLACEIDAnswer3;show\" id=\"A3show\"></a>",
    "						<a href=\"#REPLACEIDAnswer1;hide\" id=\"A1hide\"></a>",
    "						<a href=\"#REPLACEIDAnswer2;hide\" id=\"A2hide\"></a>",
    "						<a href=\"#REPLACEIDAnswer3;hide\" id=\"A3hide\"></a>",
    "						<a href=\"#REPLACEIDAnswer1;balloonFlyto\" id=\"A1go\"></a>",
    "						<a href=\"#REPLACEIDAnswer2;balloonFlyto\" id=\"A2go\"></a>",
    "						<a href=\"#REPLACEIDAnswer3;balloonFlyto\" id=\"A3go\"></a>",
    "					</div>",
    "				</div>",
    "			</main>",
    "		</div>",
    "	</body>",
    "	<script defer src=\"https://code.getmdl.io/1.3.0/material.min.js\"></script>",
    "</html>"
  ],
  "description": "GE Quiz Question"
},
